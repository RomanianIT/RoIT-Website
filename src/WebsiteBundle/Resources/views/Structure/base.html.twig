</head>

<body class="stretched sticky-responsive-pagemenu">

    <!-- Document Wrapper
    ============================================= -->
    <div id="wrapper" class="clearfix">

    {% block body %}{% endblock %}

    </div><!-- #wrapper end -->

    <!-- Go To Top
    ============================================= -->
    <div id="gotoTop" class="icon-angle-up"></div>

    <!-- JavaScripts
    ============================================= -->
    {% javascripts
        '@WebsiteBundle/Resources/public/js/jquery.js'
        '@WebsiteBundle/Resources/public/js/plugins.js'
        '@WebsiteBundle/Resources/public/js/functions.js'
        '@WebsiteBundle/Resources/public/js/jquery.city-autocomplete.min.js'
        '@WebsiteBundle/Resources/public/js/jqBootstrapValidation.js'
        '@WebsiteBundle/Resources/public/js/contact.js'
        filter='uglifyjs2'
         -%}

    <script src="{{ asset_url }}"></script>
    {% endjavascripts -%}

    {% block footer_scripts %}
    {% endblock %}

